<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SellerPage">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="450.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/logo.png" />
         </image>
      </ImageView>
      <Text layoutX="73.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Seller Page">
         <font>
            <Font name="System Bold Italic" size="42.0" />
         </font>
      </Text>
      <Button fx:id="butonDisconnect" layoutX="503.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleDisconnect" text="Disconnect" />
      <Button fx:id="buttonCreate" layoutX="23.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleRegisterAction" text="Create new account" />
      <Button fx:id="butonEditOp" layoutX="131.0" layoutY="130.0" mnemonicParsing="false" onAction="#handleEditOp" prefHeight="44.0" prefWidth="219.0" text="Edit items" />
      <Button fx:id="butonListOrders" layoutX="130.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleListOrders" prefHeight="44.0" prefWidth="219.0" text="See list of orders" />
      <Button fx:id="butonListCustomers" layoutX="131.0" layoutY="252.0" mnemonicParsing="false" onAction="#handleListCustomers" prefHeight="44.0" prefWidth="219.0" text="See list of customers" />
   </children>
</AnchorPane>
